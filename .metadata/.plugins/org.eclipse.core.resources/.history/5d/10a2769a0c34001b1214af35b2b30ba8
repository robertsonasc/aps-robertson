package atividade1;

public class RevendaComArray {
	private Produto produtos[];
	private int indice = 0;
	private int qtd;
	
	RevendaComArray(int qtdade){
		produtos = new Produto[qtdade];
		qtd = qtdade;
	}
	void inserirProduto(Produto p) {
		if(indice+1 == qtd)
			return;
		
		produtos[indice] = p;
		indice++;
	}
}
